{
  "name": "@oa2/core",
  "version": "1.1.1",
  "description": "A comprehensive, RFC-compliant OAuth 2.0 authorization server implementation in TypeScript",
  "author": "robertpitt <robertpitt1988@gmail.com>",
  "license": "MIT",
  "keywords": [
    "oauth",
    "oauth2",
    "authorization",
    "server",
    "typescript",
    "jwt",
    "pkce",
    "rfc6749"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/robertpitt/oa2.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./grants": {
      "types": "./dist/grants.d.ts",
      "import": "./dist/grants.js",
      "require": "./dist/grants.cjs"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "import": "./dist/tokens/index.js",
      "require": "./dist/tokens/index.cjs"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js",
      "require": "./dist/adapters/index.cjs"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js",
      "require": "./dist/errors.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js",
      "require": "./dist/testing/index.cjs"
    }
  },
  "scripts": {
    "build": "bunchee",
    "build:watch": "bunchee --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:security": "jest tests/security",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "bunchee",
    "prepublishOnly": "bunchee"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/hbs": "^4.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "bunchee": "^6.5.3",
    "cookie-parser": "^1.4.7",
    "eslint": "^9.31.0",
    "express": "^5.1.0",
    "hbs": "^4.2.0",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "express": "^5.0.0",
    "cookie-parser": "^1.4.0",
    "hbs": "^4.2.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "cookie-parser": {
      "optional": true
    },
    "hbs": {
      "optional": true
    }
  }
}
